<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css">
</head>

<body>

    <header>
        <partial name="_Navbar" />
        @RenderSection("Header", required: false)
    </header>
    <div class="container">

        <main role="main" class="pb-3">@RenderBody()</main>
    </div>
    <footer>
        Footer
        @RenderSection("Footer", required: false)
    </footer>
    @await RenderSectionAsync("Scripts", required: false)
    <style src="~/lib/jquery/jquery.min.js"></style>
    <style src="~/lib/font-awesome/all.min.js"></style>
    <style src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></style>
    <style src="~/js/script.js"></style>
</body>

</html>